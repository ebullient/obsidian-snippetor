{
  "name": "obsidian-snippetor",
  "version": "0.4.4",
  "description": "Generate and tweak common snippets",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "prebuild": "npm run prettier && eslint --ignore-path .gitignore src/",
    "build": "node esbuild.config.mjs production",
    "postbuild": "cp -v manifest.json README.md build",
    "test": "npx jest",
    "prettier": "prettier -w 'src/**/*.ts'",
    "preversion": "npm run test && npm run build",
    "version": "auto-changelog -p",
    "brat-notes": "run() { auto-changelog --stdout --hide-credit --hide-empty-releases --template .github/changelog.hbs -v $1 --starting-version $1  > release-notes.md; }; run"
  },
  "keywords": [
    "obsidian",
    "obsidian-md",
    "obsidian-plugin",
    "obsidian-md-plugin"
  ],
  "author": "ebullient",
  "repository": "github.com:ebullient/obsidian-snippetor",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@popperjs/core": "^2.11.2",
    "@types/chroma-js": "^2.4.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/randomcolor": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.16.0",
    "auto-changelog": "^2.4.0",
    "builtin-modules": "^3.3.0",
    "dotenv": "^16.3.1",
    "esbuild": "0.19.9",
    "esbuild-sass-plugin": "^2.16.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "obsidian": "^1.4.11",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "compare-versions": "^6.1.0",
    "eta": "^2.0.0",
    "random-word-slugs": "^0.1.6",
    "randomcolor": "^0.6.2"
  },
  "auto-changelog": {
    "backfillLimit": false,
    "commitLimit": false,
    "ignoreCommitPattern": "(ðŸ”–|ðŸ”¨|ðŸ§¹|changelog|release|Update README).*"
  }
}
