{
  "name": "obsidian-snippetor",
  "version": "0.4.3",
  "description": "Generate and tweak common snippets",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "npm run prettier && node esbuild.config.mjs production && cp manifest.json README.md build",
    "pretest": "eslint --ignore-path .gitignore src/",
    "test": "jest --passWithNoTests",
    "prettier": "prettier -w 'src/**/*.ts'",
    "preversion": "npm run build && npm run test",
    "version": "auto-changelog -p",
    "brat-notes": "run() { auto-changelog --stdout --hide-credit --hide-empty-releases --template .github/changelog.hbs -v $1 --starting-version $1  > release-notes.md; }; run"
  },
  "keywords": [
    "obsidian",
    "obsidian-md",
    "obsidian-plugin",
    "obsidian-md-plugin"
  ],
  "author": "ebullient",
  "repository": "github.com:ebullient/obsidian-snippetor",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@popperjs/core": "^2.11.2",
    "@types/chroma-js": "^2.4.0",
    "@types/node": "^14.17.21",
    "@types/randomcolor": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "auto-changelog": "^2.3.0",
    "builtin-modules": "^3.2.0",
    "dotenv": "^14.3.2",
    "esbuild": "^0.15.1",
    "esbuild-sass-plugin": "^2.0.3",
    "eslint": "^7.32.0",
    "jest": "^28.1.3",
    "obsidian": "0.16.3",
    "prettier": "^2.5.1",
    "ts-jest": "^28.0.7",
    "tslib": "^2.3.1",
    "typescript": "4.4.4"
  },
  "auto-changelog": {
    "backfillLimit": false,
    "commitLimit": false,
    "ignoreCommitPattern": "(ðŸ”–|ðŸ”¨|ðŸ§¹|changelog|release|Update README).*"
  },
  "dependencies": {
    "@types/jest": "^29.2.0",
    "compare-versions": "^4.1.3",
    "eta": "^2.0.0",
    "jest-environment-jsdom": "^29.2.2",
    "random-word-slugs": "^0.1.6",
    "randomcolor": "^0.6.2"
  }
}
